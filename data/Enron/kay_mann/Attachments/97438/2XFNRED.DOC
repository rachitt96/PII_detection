\softpage\sectd\marglsxn1440\margrsxn1440\papersize1\pgnrestart\linex0\titlepg1{\footerr\pard\s17\nowidctlpar\tqc\tx4320\tqr\tx8640\pvpara\phmrg\posxc\posy0\frmtxlrtb\plain\cs19 A-\plain\cs19\plain\cs19{\field{\*\fldinst page  \plain\cs19}{\fldrslt\plain\cs19 7\plain\cs19}}\plain\cs19\par 
